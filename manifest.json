{
 "manifest_version": 2,
 "name": "Wesplash",
 "description": "Random background images with weather information",
 "version": "1.0",
 "author": "Vivek Shaw",
"browser_action": {
   "default_title": "Wesplash"
 },
"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
 "permissions": ["activeTab",
 "geolocation",
 "*://*.api.unsplash.com//*",
 "*://*.api.openweathermap.org//*",
 "https://ajax.googleapis.com/"]
}
